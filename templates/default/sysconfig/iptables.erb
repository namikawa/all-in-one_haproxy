*filter
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
<%- if !node["#{@file}"].nil? and node["#{@file}"].size != 0 %>
  <%- for id, filter in node["#{@file}"]['allow']['src'] do %>
    <%- for addr in filter['address'] do %>
-A INPUT -s <%= addr %> -p tcp -m tcp --dport <%= filter['dport'] %> -j ACCEPT
    <%- end %>
-A INPUT -p tcp -m tcp --dport <%= filter['dport'] %> -j REJECT
  <%- end %>
<%- end %>
COMMIT

